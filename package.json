{
  "name": "loginApp",
  "version": "1.0.0",
  "description": "A minimal login app with JSON web tokens",
  "main": "index.js",
  "engines": {
    "node": "8.1.1",
    "npm": "5.0.3"
  },
  "scripts": {
    "start": "node src/index.js",
    "server": "nodemon --exec babel-node src/index.js",
    "client": "npm run start --prefix src/client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "keywords": [],
  "author": "FoteinosMerg <foteinosmerg@gmail.com> (https://github.com/FoteinosMerg)",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "nodemon": "^1.18.4"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parse": "^0.1.0",
    "concurrently": "^4.0.1",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.2.17",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "react-scripts": "^2.0.4",
    "validator": "^10.8.0"
  }
}
